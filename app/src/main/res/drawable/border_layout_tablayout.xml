<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:start="-3dp"
        android:end="-3dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape
            android:layout_width="match_parent"
            android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#75A488" />
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>